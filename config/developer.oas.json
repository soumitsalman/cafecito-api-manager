{
  "openapi": "3.1.0",
  "info": {
    "title": "Developer API",
    "version": "1.0.0"
  },
  "paths": {
    "/v1/developer/api-key": {
      "x-zuplo-path": {
        "pathMode": "open-api"
      },
      "post": {
        "summary": "Create API Key",
        "description": "Creates an API key for the authenticated developer",
        "x-zuplo-route": {
          "corsPolicy": "anything-goes",
          "handler": {
            "export": "default",
            "module": "$import(./modules/create-api-keys)",
            "options": {}
          },
          "policies": {
            "inbound": [
              "clerk-jwt-auth-inbound",
              "infra-rate-limit-inbound"
            ]
          }
        },
        "operationId": "create-developer-api-key",
        "x-internal": true
      }
    },
    "/v1/developer/webhooks/clerk": {
      "x-zuplo-path": {
        "pathMode": "open-api"
      },
      "post": {
        "summary": "Clerk Webhook",
        "description": "Receives Clerk webhook events for user lifecycle changes (deletion, metadata updates). Syncs API key consumers accordingly.",
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "default",
            "module": "$import(./modules/clerk-webhook)",
            "options": {}
          },
          "policies": {
            "inbound": [
              "infra-rate-limit-inbound"
            ]
          }
        },
        "operationId": "clerk-webhook-handler",
        "x-internal": true
      }
    }
  }
}