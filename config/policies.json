{
  "policies": [
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": true,
          "cacheTtlSeconds": 300
        }
      },
      "name": "zuplo-api-key-inbound",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "ClerkJwtInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": true,
          "frontendApiUrl": "$env(CLERK_FRONTEND_API_URL)",
          "oAuthResourceMetadataEnabled": true
        }
      },
      "name": "clerk-jwt-auth-inbound",
      "policyType": "clerk-jwt-auth-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/gate-auth)"
      },
      "name": "gate-auth",
      "policyType": "custom-code-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "user",
          "requestsAllowed": 10,
          "timeWindowMinutes": 1,
          "identifier": {
            "export": "rateLimit",
            "module": "$import(./modules/tiered-rate-limit)"
          }
        }
      },
      "name": "paiduser-rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    },
    {
      "handler": {
        "export": "QuotaInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowances": {
            "requests": 50000
          },
          "period": "monthly",
          "quotaBy": "user",
          "quotaOnStatusCodes": "200-399"
        }
      },
      "name": "freeuser-quota-inbound",
      "policyType": "quota-inbound"
    },
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "X-API-KEY",
              "value": "$env(BACKEND_API_KEY)"
            }
          ]
        }
      },
      "name": "backend-api-key-inbound",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "ip",
          "requestsAllowed": 100,
          "timeWindowMinutes": 1
        }
      },
      "name": "infra-rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "user",
          "requestsAllowed": 100,
          "timeWindowMinutes": 1
        }
      },
      "name": "freeuser-rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    }
  ]
}